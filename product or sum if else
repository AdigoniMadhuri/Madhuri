#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a,b;
    scanf("%d\n%d\n",&a,&b);
     int r=a*b;
       if(r<=1000)
       {
  
        printf("%d\n",r);
    }  
    else
    {
        printf("%d\n",a+b);
    }
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
