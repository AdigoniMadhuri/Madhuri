import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args)
    {
        
    Scanner s =new Scanner(System.in);
        int a=s.nextInt();
        int b=s.nextInt();
     int r=a*b;
       
       if(r<=1000)
       {
          System.out.printf("%d\n",r);
    }  
    else
    {
        System.out.printf("%d\n",a+b);
    }
       s.close(); 
    }
}
