import java.io.*;
import java.util.*;

public class Solution
{
    public static void main(String[] args) 
    {
        Scanner s=new Scanner(System.in);
        int Q=s.nextInt();
        int P=s.nextInt();
    int r1=Q;
    System.out.printf("Quantity Ordered %d\n",r1);
    int r2=P;
    System.out.printf("Price per item is Rs.%d\n",r2);
    int t=r1*r2;
    System.out.printf("Total Amount = Rs.%d\n",t);
    double G=(double)(t-410);
    System.out.printf("GST - Rs.%.1f\n",G);
    double r3=(double)(t+G);
    System.out.printf("Net Amount Payable is Rs.%.1f\n",r3);
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
    }
}
